{
  "name": "vue-lib-sandbox",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  },
  "files": ["dist"],
  "main": "./dist/vue-lib-sandbox.umd.js",
  "module": "./dist/vue-lib-sandbox.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-lib-sandbox.es.js",
      "require": "./dist/vue-lib-sandbox.umd.js"
    }
  },
  "types": "./dist/types/index.d.ts"
}
