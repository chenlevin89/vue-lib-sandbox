{
  "name": "vue-lib-sandbox",
  "private": true,
  "version": "0.0.20",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly && node inject-css.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "replace": "^1.2.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-static-copy": "^0.6.0",
    "vue-tsc": "^0.35.0"
  },
  "main": "./index.js",
  "module": "./index.js",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./components/button": {
      "types": "./components/button/Button.vue.d.ts",
      "import": "./components/button/Button.vue.js"
    },
    "./components/input": {
      "types": "./components/input/Input.vue.d.ts",
      "import": "./components/input/Input.vue.js"
    }
  },
  "typesVersions": {
    "*": {
      "components/button": [
        "./components/button/Button.vue.d.ts"
      ],
      "components/input": [
        "./components/input/Input.vue.d.ts"
      ]
    }
  }
}
